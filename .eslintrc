{
    "parser":"babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "quotes": "off",
        "no-unused-vars": "off",
        "prefer-const": "off"
    },
    "extends": ["eslint:recommended", "google"]
}